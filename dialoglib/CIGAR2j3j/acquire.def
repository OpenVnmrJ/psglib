# CIGAR

{
   label: CIGAR Scans per inc:
   choice:   "8" "4" "16" "32" 
   choice_value:  "nt=8" "nt=4" "nt=16" "nt=32" 
   output: $choice_value
   remark: Set number of scans 
}

{
   label: CIGAR number of inc:
   choice: "400" "200" 256" "512" 
   choice_value:  "ni=400" "ni=200" "ni=256" "ni=512" 
   output: $choice_value  sinebell
   remark: Set number of inc 
}
 
{
   label: Carbon Spectral Width (ppm):
   choice: "-15 -> 225" "-10 -> 190" "-10 -> 160" "-20 -> 240"
   choice_value: "setsw1(dn,225,-15):dof" "setsw1(dn,190,-10):dof" "setsw1(dn,160,-10):dof" "setsw1(dn,240,-20):dof"
   output: $choice_value sinebell
   remark: Set carbon spectral window 
   show: dn = 'C13'
}

{
   label: CIGAR max coupling constant:
   choice: "10Hz" "8Hz" "12Hz"
   choice_value: "jnmax=10" "jnmax=8" "jnmax=12" "jnmax=15"
   output: $choice_value
   remark: Set coupling constant for CIGAR
}

{
   label: CIGAR min coupling constant:
   choice: "5Hz" "3Hz" "4Hz" "2Hz"
   choice_value: "jnmin=5" "jnmin=3" "jnmin=4" "jnmin=2"
   output: $choice_value
   remark: Set coupling constant for CIGAR
}

{
   button(1): OK
   exec(1):   SAVE CLOSE
   button(2): Reset
   exec(2):   RESET
}

