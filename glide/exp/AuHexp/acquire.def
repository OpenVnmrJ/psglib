# AuH2D

{
   label: PROTON Spectral Width (ppm):
   choice: "-2->14" "-1->11" "-0.5->9.5" "0.5->8.5"
   choice_value: "'setsw(14,-2)'" "'setsw(11,-1)'" "'setsw(9.5,-.5)'" "'setsw(8.5,0.5)'"
   rtoutput: "Autowrmac('PROTON','acq',$choice_value)"
   output: dg  
   remark: "Select 1H spectral window"
}

{
   label: Minimize SW?:
   choice: "NO" "Auto" "Manual"
   choice_value: "'MinSW=`off`'" "'MinSW=`auto`'" "'MinSW=`manual`'"
   rtoutput: "Autowrmac('PROTON','acq',$choice_value)"
   remark:
}

{
   label: PROTON scans:
   choice: "8" "16" "32"
   choice_value:  "'nt=8'" "'nt=16'" "'nt=32'"
   rtoutput: "Autowrmac('PROTON','acq',$choice_value)"
   remark: Set number of scans for 1D proton
}

{
   label: PROTON Relaxation Delay (sec):
   choice: "1" "2" "5" "25"
   choice_value:  "'d1=1'" "'d1=2'" "'d1=5'" "'d1=25'"
   rtoutput: "Autowrmac('PROTON','acq',$choice_value)"
   remark: Proton Relaxation Delay - for 1D ONLY
}

{
   label: PROTON Pulse Angle:
   input: 45
   choice:      "Default" "Set"
   choice_value:  "'pw(45)'" "'pw($input)'"
   rtoutput: "Autowrmac('PROTON','acq',$choice_value)"
   remark: Enter the pulse angle in degrees
}


{
   label: Select Experiments in addition to PROTON: 
      
   check_button(1) 		: "COSY"
   check_set_value(1) 		: "'COSY'"
   check_unset_value(1)		: "'COSY'"
   check_set_rtoutput(1) 	: AutoAddEXP($value(1))
   check_unset_rtoutput(1) 	: AutoDelEXP($value(1))
   
