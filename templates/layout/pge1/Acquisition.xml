<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Acquisition" element="pages" type="acquisition" >
  <group size="800 200"
    style="PlainText"
    label="Acquisition"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Acquisition"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="270 140" size="240 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <check loc="170 0" size="20 20"
        style="Label4"
        vq="gain"
        vc="on('gain')"
        vc2="off('gain')"
        set="on('gain'):$VALUE if ($VALUE=-1) then $VALUE=0 endif"
        show="on('gain'):$VALUE if ($VALUE&gt;-1) then $VALUE=1 endif"
        />
      <textmessage loc="200 0" size="40 20"
        style="PlainText"
        label=" "
        vq="gain"
        set="on('gain'):$gainon if ($gainon&gt;-1) then if ($gainon=0) then $VALUE='auto on' else $VALUE='' endif else $VALUE='' endif"
        />
      <label loc="10 0" size="110 20"
        style="Label4"
        label="Receiver Gain:"
        justify="Left"
        />
      <entry loc="120 0" size="50 20"
        style="Label4"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        disable="Grayed out"
        />
    </group>
    <entry loc="340 0" size="110 20"
      style="Label3"
      vq="seqfil"
      vc="seqfil='$VALUE' "
      set="$VALUE=seqfil"
      show="on('seqfil'):$VALUE"
      disable="Grayed out"
      />
    <label loc="270 0" size="60 20"
      style="Label3"
      label="Sequence:"
      justify="Left"
      />
    <group loc="0 0" size="260 160"
      style="Heading4"
      label="Acquisition"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Data"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="202 120" size="50 20"
        style="PlainText"
        label="bs on"
        vq="bs"
        set="on('bs'):$bson if ($bson&gt;-1) then if ($bson=1) then $VALUE='bs on' else $VALUE='bs off' endif else $VALUE='' endif"
        />
      <check loc="172 120" size="20 20"
        style="Label4"
        vq="bs $VALUE=-1"
        vc="on('bs')"
        vc2="off('bs')"
        set="on('bs'):$VALUE if ($VALUE=-1) then $VALUE=0 endif"
        show="on('bs'):$VALUE if ($VALUE&gt;-1) then $VALUE=1 endif"
        />
      <textmessage loc="202 100" size="50 20"
        style="Label4"
        label="0"
        vq="ct"
        set="$VALUE=ct"
        show="on('ct'):$VALUE"
        />
      <label loc="172 100" size="30 20"
        style="Label4"
        label="ct:"
        justify="Center"
        />
      <menu loc="192 80" size="60 20"
        style="Label4"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        show="on('d1'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <menu loc="192 40" size="60 20"
        style="Label4"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        show="on('at'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <label loc="12 20" size="100 20"
        style="Label4"
        label="Spectral Width:"
        justify="Left"
        />
      <entry loc="112 20" size="80 20"
        style="Label4"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        show="on('sw'):$VALUE "
        disable="Grayed out"
        />
      <menu loc="192 20" size="60 20"
        style="Label4"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        show="on('sw'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="MHz"
          chval="MHz"
          />
        <mlabel 
          label="kHz"
          chval="kHz"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="112 140" size="60 20"
        style="Label4"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="on('ss'):$VALUE "
        disable="Grayed out"
        />
      <entry loc="112 120" size="60 20"
        style="Label4"
        vq="bs"
        vc="$bs =bs bs=$VALUE vs=vs*($bs/bs)"
        set="$VALUE=bs"
        show="on('bs'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="112 100" size="60 20"
        style="Label4"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        show="on('nt'):$VALUE "
        disable="Grayed out"
        />
      <entry loc="112 80" size="80 20"
        style="Label4"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="on('d1'):$VALUE "
        disable="Grayed out"
        />
      <entry loc="112 60" size="80 20"
        style="Label4"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        show="on('np'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="112 40" size="80 20"
        style="Label4"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        show="on('at'):$VALUE "
        disable="Grayed out"
        />
      <textmessage loc="122 0" size="60 20"
        style="Heading4"
        label="N15"
        vq="tn"
        set="$VALUE=tn"
        />
      <label loc="12 140" size="100 20"
        style="Label4"
        label="Steady State:"
        justify="Left"
        />
      <label loc="12 120" size="100 20"
        style="Label4"
        label="Block Size:"
        justify="Left"
        />
      <label loc="12 100" size="100 20"
        style="Label4"
        label="Total Scans:"
        justify="Left"
        />
      <label loc="12 80" size="100 20"
        style="Label4"
        label="Recycle Delay:"
        justify="Left"
        />
      <label loc="12 60" size="100 20"
        style="Label4"
        label="Complex Points:"
        justify="Left"
        />
      <label loc="12 40" size="100 20"
        style="Label4"
        label="Acquisition Time:"
        justify="Left"
        />
      <label loc="12 0" size="100 20"
        style="Heading4"
        label="Acquisition:"
        justify="Left"
        />
    </group>
  </group>
</template>
