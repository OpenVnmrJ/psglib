/*
   Phasecycle for hnco2.c pulse sequence
   Second revision
   256-step phasecycle
   S. Farmer   1-15-91
*/
 t1 = 0						/* 1H	*/
 t2 = 0						/* 1H	*/
 t3 = 0 0 2 2					/* 15N  */
 t4 = {1 3}32					/* 1H   */
 t5 = 0	2					/* 13C0	*/
 t6 = {0 1 2 3}64				/* 13Ca */
 t7 = {0 1 2 3}64				/* 15N  */
 t8 = {2 3 0 1}64				/* 13Ca	*/
 t9 = {0 2}4					/* 13CO */
t11 = {0 2}16					/* 1H   */
t12 = {0 1}8					/* 1H   */
t13 = 0						/* 15N  */
t14 = 1                                         /* 1H purge  */
t15 = 0 2 2 0 2 0 0 2 2 0 0 2 0 2 2 0
      2 0 0 2 0 2 2 0 0 2 2 0 2 0 0 2
      2 0 0 2 0 2 2 0 0 2 2 0 2 0 0 2
      0 2 2 0 2 0 0 2 2 0 0 2 0 2 2 0
      2 0 0 2 0 2 2 0 0 2 2 0 2 0 0 2
      0 2 2 0 2 0 0 2 2 0 0 2 0 2 2 0
      0 2 2 0 2 0 0 2 2 0 0 2 0 2 2 0
      2 0 0 2 0 2 2 0 0 2 2 0 2 0 0 2		/* rcvr */
