"       Modified by D. Rice 10/02/00                              "

$num = size('grad_hs')
dac_hs = 0            " clear old array "
$i = 1
while ($i <= $num) do
  dac_hs[$i] = grad_hs[$i] / grad_p_coef
  grad_hs[$i] = dac_hs[$i] * grad_p_coef
  $i = $i + 1
  endwhile
